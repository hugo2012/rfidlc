{
    "_version": "1.12.0",
    "sap.app": {
        "id": "com.bosch.rbf0.lo.rfidlc",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.1.0"
        },
        "dataSources": {
            "YP3_RFID_LOAD_CONTROL_SRV": {
                "uri": "/sap/opu/odata/RB4R/LO_RFID_LOAD_MONITOR_API/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        },
        "sourceTemplate": {
            "id": "sap.ui.ui5-template-plugin.2masterdetail",
            "version": "1.52.7",
            "toolsId": "e810b3a2-e183-42b4-af32-07ae3690ba0d"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": false
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "config": {
            "sapFiori2Adaptation": true
        },
        "rootView": {
            "viewName": "com.bosch.rbf0.lo.rfidlc.view.App",
            "type": "XML",
            "async": true,
            "id": ""
        },
        "dependencies": {
            "minUI5Version": "1.108.30",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.bosch.rbf0.lo.rfidlc.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "None",
                    "useBatch": true
                },
                "dataSource": "YP3_RFID_LOAD_CONTROL_SRV",
                "preload": true
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewPath": "com.bosch.rbf0.lo.rfidlc.view",
                "controlId": "idAppControl",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "controlAggregation": "pages",
                "clearControlAggregation": false,
                "async": true
            },
            "routes": [
                {
                    "name": "startup",
                    "pattern": "",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "startup"
                    ]
                },
                {
                    "name": "monitor",
                    "pattern": "{WarehouseNo}/{gate}",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "monitor"
                    ]
                }
            ],
            "targets": {
                "startup": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearAggregation": true,
                    "viewName": "Startup",
                    "viewLevel": 1,
                    "viewId": "startup"
                },
                "monitor": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearAggregation": true,
                    "viewName": "Monitor",
                    "viewId": "monitor",
                    "viewLevel": 2
                },
                "notFound": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearAggregation": true,
                    "viewName": "NotFound",
                    "viewLevel": 5,
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/rbf0/rfidlc/webapp",
        "_version": "1.1.0"
    }
}