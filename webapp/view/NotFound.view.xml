<mvc:View controllerName="com.bosch.rbf0.lo.rfidlc.NotFound" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<MessagePage id="page" title="{i18n>notFoundTitle}" text="{i18n>notFoundText}" icon="{sap-icon://document}" description=""
